    </tr>

    <tr>
        <td>
            <div class="form-group">
                <label>Overtime :</label> <br />
                <label><b>@ViewBag.x</b></label>
            </div>
        </td>
        <td>
            <div class="form-group">
                <label>Jam Overtime :</label> <br />
